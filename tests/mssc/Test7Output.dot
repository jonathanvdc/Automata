digraph dfa {
    rankdir = LR;
    __start [style = invis, shape = point];
    __start -> "{A}" [ label = "start" ];
    node [shape = doublecircle];
    node [shape = circle];
    "{A}" -> "{}" [ label = "n" ];
    "{A}" -> "{A}" [ label = "a" ];
    "{}" -> "{}" [ label = "n" ];
    "{}" -> "{}" [ label = "a" ];
}
